<h1><%= @league.name %></h1>
<h5><%= @company.name %></h5>


<h3>Standings:</h3>
<table>
  <% @standings_hash.each do |team, wins| %>
    <tr>
      <td><%= link_to "#{team.name}", "/teams/#{team.id}" %></td>
      <td><%= wins %>-<%= @weeks_in_season_count - wins %></td>
    </tr>
  <% end %>
</table>

